[
    {
        "id": "monday_exercises",
        "day": "Pazartesi",
        "muscle_group": "Omuz",
        "exercises": [
            {
                "id": "m1",
                "name": "Shoulder Press Machine",
                "set": "4x12",
                "description": "Vücutta omuz, sırt ve kol kaslarının gelişmesi ve buna bağlı olarak vücudun şekillenmesini sağlamaktadır.",
                "media": "public/gifs/shoulder_press_machine.gif"
            },
            {
                "id": "m2",
                "name": "Dumbell Lateral Raise",
                "set": "4x12",
                "description": "Hareketin çalıştırdığı bölgeler yan omuz, omuz başları ve trapez kaslarıdır.",
                "media": "public/gifs/dumbbell_lateral_raise.gif"
            },
            {
                "id": "m3",
                "name": "Arnold Press",
                "set": "4x12",
                "description": "Bu hareket ikincil kas grupları olan trapez ve triceps bölgesindeki kaslarında aktif bir şekilde çalışmasını sağlamaktadır.",
                "media": "public/gifs/arnold_press.gif"
            },
            {
                "id": "m4",
                "name": "Dumbell Front Raise",
                "set": "4x12",
                "description": "Bu hareket aynı zamanda up pectoralis major kasını çalıştırmaktadır.",
                "media": "public/gifs/dumbbell_front_raise.gif"
            },
            {
                "id": "m5",
                "name": "Facepull Machine",
                "set": "4x12",
                "description": "Omuzlarımızı özellikle de arka omuz başlarını ve arka deltoidleri çalıştırıp güçlendirip ve dayanıklı bir hale getiren egzersizdir. Ayrıca ön kol kaslarımızı da geliştirmektedir.",
                "media": "public/gifs/facepull.gif"
            },
            {
                "id": "m6",
                "name": "Dumbell Shrag Raise",
                "set": "4x12",
                "description": "Çalıştırdığı kaslar arasında en dikkat çeken trapez kasıdır hatta bu hareketin özellikle trapez kasını çalıştırmak için yapıldığı söylenebilir. Aynı zamanda boyundan aşağı doğru duran sırt kaslarını da çalıştırmaktadır.",
                "media": "public/gifs/dumbbell_shrug.gif"
            }
        ]
    },
