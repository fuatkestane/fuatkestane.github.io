        "exercises": [
            {
                "id": "w1",
                "name": "Squad",
                "set": "4x12",
                "description": "Squat birincil olarak vücudun en hacimli kas gruplarından biri olan ön bacak bölgesini yani quadriceps kasını, arka bacak kaslarını ve kalça kaslarını çalıştırır. ",
                "media": "public/gifs/squat.gif"
            },
            {
                "id": "w2",
                "name": "Leg Press",
                "set": "4x12",
                "description": "Kişinin bacaklarını kullanarak bir ağırlık veya direnci ileri ittiği bileşik bir ağırlık antrenmanı egzersizidir.",
                "media": "public/gifs/leg_press.gif"
            },
            {
                "id": "w3",
                "name": "Leg Extensions",
                "set": "4x12",
                "description": "Leg extension, bacaklardaki kuadriseps kasını hedef alan bir direnç ağırlık antrenmanı egzersizidir.",
                "media": "public/gifs/leg_extensions.gif"
            },
            {
                "id": "w4",
                "name": "Leg Curl",
                "set": "4x12",
                "description": "Seated Leg curl hareketi arka bacak kaslarınızı çalıştırarak güçlenmesini ve antrenman şiddet ve programınıza göre büyümesini veya daha sıkı arka bacaklara sahip olmanızı sağlayacaktır.",
                "media": "public/gifs/leg_curl.gif"
            },
            {
                "id": "w5",
                "name": "Leg Abduction",
                "set": "4x12",
                "description": "Kol, bacak, el ve ayak parmaklarının merkezden uzaklaştırılarak hareket ettirilmesi anlamına gelmektedir. ",
                "media": "public/gifs/leg_abduction.gif"
            },
            {
                "id": "w6",
                "name": "Leg Adduction",
                "set": "4x12",
                "description": "Bacaklar açık halde bir yüke maruz bırakılan bu makinede, özellikle bacak kaslarının iç kısımda kalan kaslarını aktif olarak çalıştırmaktadır.",
                "media": "public/gifs/leg_adduction.gif"
            },
